<script>
	define('QuickView.Config', function () {
		return {
		    domNode: '.quickview-container',
		    saveForLaterDomNode: '.save-for-later',
			scene7: {
				imageServer: 'http://images.asos-media.com/',
				videoServer: 'http://video.asos-media.com/'
			},
			localisations: {
				incorrectSelection: 'Выберите имеющийся в наличии размер и цвет',
